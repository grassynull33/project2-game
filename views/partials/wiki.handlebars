<!-- Wiki -->
<section id="wiki" class="five">
  <div class="container">

    <header>
      <h2>Wiki</h2>
      <h3 id="item-counter'">{{wikiCount}} <small>items total</small></h3>
    </header>
    <div class="container">
      <ul class="wiki-list">
        {{#each wiki}}
        <li class="wiki-list-item legendary-{{this.isLegendary}}">
          <h4>{{this.name}}</h4>
          <span class="badge rarity legendary-{{this.isLegendary}}">Legendary</span>
          <p>{{this.description}}</p>

        </li>
        {{/each}}
      </ul>
    </div>

  </div>
</section>